{
  "name": "chai-calling-with",
  "version": "1.0.0",
  "description": "Chai assertions for testing exceptions in a readable way",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha $(find . -name \"*.spec.js\" -not -path \"./node_modules/*\")",
    "code-style": "eslint $(find . -name \"*.js\" -not -path \"./node_modules/*\")",
    "validate-commit-msg": "validate-commit-msg",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bankyadam/chai-calling-with.git"
  },
  "author": "Emarsys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bankyadam/chai-calling-with/issues"
  },
  "pre-commit": [
    "code-style",
    "validate-commit-msg"
  ],
  "peerDependencies": {
    "chai": ">= 2.1.2 <= 4"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.0.1",
    "eslint-config-emarsys": "5.1.0",
    "eslint-plugin-no-only-tests": "2.3.1",
    "eslint-plugin-security": "1.4.0",
    "mocha": "6.1.4",
    "pre-commit": "1.2.2",
    "semantic-release": "15.13.17",
    "validate-commit-message": "3.0.1"
  },
  "engines": {
    "node": "6.7.0"
  },
  "homepage": "https://github.com/bankyadam/chai-calling-with#readme"
}
